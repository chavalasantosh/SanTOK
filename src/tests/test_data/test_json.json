{
  "name": "SanTOK Tokenizer",
  "version": "1.0.0",
  "features": [
    "space tokenization",
    "word tokenization", 
    "character tokenization",
    "grammar tokenization",
    "subword tokenization",
    "byte tokenization"
  ],
  "compression": {
    "rle": true,
    "pattern": true,
    "frequency": true,
    "adaptive": true
  },
  "reversibility": true,
  "unique_ids": true
}
