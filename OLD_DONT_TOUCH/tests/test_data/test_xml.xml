<?xml version="1.0" encoding="UTF-8"?>
<root>
  <tokenizer name="Krishna">
    <features>
      <feature>Space Tokenization</feature>
      <feature>Word Tokenization</feature>
      <feature>Character Tokenization</feature>
      <feature>Grammar Tokenization</feature>
      <feature>Subword Tokenization</feature>
      <feature>Byte Tokenization</feature>
    </features>
    <compression>
      <algorithm>RLE</algorithm>
      <algorithm>Pattern</algorithm>
      <algorithm>Frequency</algorithm>
      <algorithm>Adaptive</algorithm>
    </compression>
  </tokenizer>
</root>
